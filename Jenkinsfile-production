pipeline {
    agent {
        docker {
            image 'node:14-alpine3.15'
            args '-p 8080:8080'
        }
    }
     environment {
            STAGING = 'true'
			PRODUCTION = 'true'

        }
    stages {
        stage('Deploy') {
            steps {
                sh 'npm install'
            }
        }
	}

    }
}